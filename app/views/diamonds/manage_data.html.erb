<h1>Data management page</h1>

<div class= "container">
	<%= form_tag diamonds_import_path, multipart: true do %>
		<%= file_field_tag :file %>
		<%= submit_tag "CSVファイルインポート", :class => "btn btn-primary btn-lg btn-block user_margin1" %>
    <% end %>
    
    <%= button_to "チャート用データテーブル作成", { :action => "run"}, :class => "btn btn-primary btn-lg btn-block user_margin1" %>
    
    <%= button_to "Daily価格テーブル作成", { :action => "make_list"}, :class => "btn btn-primary btn-lg btn-block user_margin1" %>
    
    <div>
        <btn><%= link_to "Admin管理ページ", admin_root_path, :class => "btn btn-primary btn-lg user_margin1" %></btn>
    </div>
    
</div>

