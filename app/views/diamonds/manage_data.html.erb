<h1>Data management page</h1>

<div class= "container">
	<%= #form_tag import_diamonds_path, multipart: true do %>
	<%= form_tag {:action => 'import'}, :remote => true do %>
		<%= file_field_tag :file %>
		<%= submit_tag "インポート" %>
    <% end %>
    
    <%= button_to "データテーブル作成", { :action => "run"}, {:remote => true} %>
    
</div>

